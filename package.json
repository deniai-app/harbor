{
  "name": "deni-ai-harbor",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "bun apps/api/src/index.ts",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format": "oxfmt .",
    "format:check": "oxfmt --check ."
  },
  "devDependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20260223.1",
    "@workspace/typescript-config": "workspace:*",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "turbo": "^2.8.1"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.3.9"
}
